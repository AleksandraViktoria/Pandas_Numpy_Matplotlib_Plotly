# Matrix Multiplication 

import numpy as np 

a = np.array([[1, 2],[3, 4]])

b = np.array([[11, 12],[13, 14]])

print(np.dot(a, b))

# Answer:

# [[37 40]

# [85 92]]

#----------------Same Results--------------------

import numpy as np 

a = np.array([[1, 2],[3, 4]])

b = np.array([[11, 12],[13, 14]])

print(a @ b)

# Answer:

# [[37 40]
# [85 92]]
